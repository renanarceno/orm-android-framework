<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app">

    <application
        android:name=".TestApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="tccframework.dbVersion"
            android:value="1" />
        <meta-data
            android:name="tccframework.dbName"
            android:value="teste.db" />

        <meta-data
            android:name="tccframework.dbAssets.dbCreateFolder"
            android:value="db/create" />

        <meta-data
            android:name="tccframework.dbAssets.dbUpdateFolder"
            android:value="db/update" />

        <activity android:name=".view.LoginView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>